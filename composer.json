{
  "name": "baks-dev/files-cdn",
  "description": "Модуль CDN файловых ресурсов",
  "homepage": "https://github.com/baks-dev/files-cdn",
  "keywords": [
    "bundle",
    "symfony",
    "files",
    "resources",
    "cdn",
    "plugin",
    "library",
    "module",
    "baksdev"
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-gd": "*",
    "symfony/monolog-bundle": "*",
    "symfony/security-bundle": "*",
    "symfony/expression-language": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BaksDev\\Files\\Cdn\\": ""
    }
  },

  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
