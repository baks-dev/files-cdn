{
  "name": "baks-dev/files-cdn",
  "description": "Модуль CDN айловых ресурсов",
  "homepage": "https://github.com/baks-dev/files-cdn",
  "keywords": ["library", "symfony", "files", "resources", "cdn", "plugin", "library", "module", "baksdev"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  
  "require": {
    "php": ">=8.1",
    "ext-gd": "*"
  },

  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/test-pack": "^1.0"
  },

  "autoload": {
    "psr-4": {
      "BaksDev\\Files\\Cdn\\": ""
    }
  },

  "autoload-dev": {
    "psr-4": {
      "BaksDev\\Files\\Cdn\\Tests\\": "Tests/"
    }
  },

  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
